# Google Analytics API Integration
#
# - Use the service account email generated by Google console to give access to
#   profiles that will be reported to StackState.
# - Be sure StackState Agent can read key file.
#
init_config:
    min_collection_interval: 55
    key_file_location: /etc/sts-agent/conf.d/key.json

instances:
  - profile: ga:12345678
    metrics:
      - rt:activeUsers
    dimensions:
      - rt:minutesAgo
    filters:
     - rt:pagePath==/foo/bar
    tags:
     - env:production
